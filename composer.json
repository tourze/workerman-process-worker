{
  "name": "tourze/workerman-process-worker",
  "description": "Workerman 进程管理器，支持外部进程的监控和事件处理",
  "license": "MIT",
  "require": {
    "symfony/event-dispatcher": "^7.3",
    "symfony/event-dispatcher-contracts": "^3",
    "workerman/workerman": "^5.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/framework-bundle": "^7.3",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-symfony-unit-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Workerman\\ProcessWorker\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Workerman\\ProcessWorker\\Tests\\": "tests"
    }
  }
}